{
  "name": "community-twitch-development",
  "version": "1.0.0",
  "description": "A web app that helps Twitch streamers collaborate with viewers on a development project.",
  "main": "server.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run dev-build && npm run dev-server",
    "prod": "npm run prod-build && npm run prod-server",
    "dev-server": "cross-env NODE_ENV=development nodemon ./server.js",
    "prod-server": "cross-env NODE_ENV=production node ./server.js",
    "dev-build": "webpack --config ./webpack.dev.config.js",
    "prod-build": "webpack -p --config ./webpack.prod.config.js",
    "test": "cross-env NODE_ENV=test mocha ./test/**/*.spec.js --compilers js:babel-register"
  },
  "keywords": [
    "twitch",
    "react",
    "node"
  ],
  "author": "Justin Clagg",
  "license": "MIT",
  "homepage": "",
  "engines": {
    "node": "7.4.0",
    "npm": "4.0.5"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cross-env": "^3.1.4",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "isomorphic-fetch": "^2.2.1",
    "linkifyjs": "^2.1.4",
    "material-ui": "^0.16.6",
    "mongoose": "^4.7.6",
    "passport": "^0.3.2",
    "passport-gitlab2": "^2.2.0",
    "passport-twitchtv": "^0.1.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.0",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "socket.io-redis": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.26.1",
    "dotenv-webpack": "^1.3.0",
    "eslint": "^3.9.0",
    "eslint-plugin-react": "^6.4.1",
    "fetch-mock": "^5.8.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "img-loader": "^1.3.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "morgan": "^1.7.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.10.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.0.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
